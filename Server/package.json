    "socket.io": "^2.0.3",
    "typescript-collections": "^1.2.3"
  },
  "devDependencies": {
    "@types/express": "^4.0.37",
    "@types/lodash": "^4.14.74",
    "@types/p2": "0.7.30",
    "@types/socket.io": "^1.4.30",
    "concurrently": "^3.5.0",
    "nodemon": "^1.12.0",
    "tslint": "^5.7.0",
    "typescript": "^2.5.2"
  }
}